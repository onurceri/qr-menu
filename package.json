{
  "name": "qr-menu",
  "private": true,
  "version": "0.0.1",
  "type": "module",
  "scripts": {
    "dev:server": "cross-env NODE_ENV=development tsx watch server/index.ts",
    "dev:client": "vite",
    "dev": "concurrently \"npm run dev:server\" \"npm run dev:client\"",
    "build": "rm -rf dist && npm run build:client && npm run build:server",
    "build:client": "vite build",
    "build:server": "tsc -p tsconfig.server.json && node scripts/copy-models.js",
    "start": "cross-env NODE_ENV=production node -r dotenv/config dist/server/index.js",
    "lint": "eslint . --max-warnings=0",
    "preview": "vite preview",
    "prepare": "husky install",
    "lint-staged": "lint-staged"
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ]
  },
  "dependencies": {
    "@hello-pangea/dnd": "^16.6.0",
    "@tanstack/react-query": "^5.59.20",
    "@tanstack/react-router": "^1.81.4",
    "@types/express-mongo-sanitize": "^1.3.2",
    "@types/helmet": "^0.0.48",
    "cloudinary": "^2.5.1",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.18.2",
    "express-mongo-sanitize": "^2.2.0",
    "express-rate-limit": "^7.4.1",
    "express-validator": "^7.2.0",
    "firebase": "^10.8.0",
    "firebase-admin": "^13.0.0",
    "helmet": "^8.0.0",
    "i18next": "^23.16.5",
    "i18next-browser-languagedetector": "^8.0.0",
    "leaflet": "^1.9.4",
    "lucide-react": "^0.344.0",
    "mongodb": "^6.10.0",
    "mongoose": "^8.8.1",
    "multer": "^1.4.5-lts.1",
    "openai": "^4.72.0",
    "qrcode.react": "^3.1.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-i18next": "^15.1.1",
    "react-leaflet": "^4.2.1",
    "react-router-dom": "^6.22.1",
    "uuid": "^11.0.3"
  },
  "devDependencies": {
    "@eslint/js": "^9.9.1",
    "@jridgewell/sourcemap-codec": "^1.5.0",
    "@types/cors": "^2.8.17",
    "@types/dotenv": "^6.1.1",
    "@types/express": "^5.0.0",
    "@types/leaflet": "^1.9.14",
    "@types/multer": "^1.4.12",
    "@types/node": "^22.9.0",
    "@types/react": "^18.3.5",
    "@types/react-dom": "^18.3.0",
    "@vitejs/plugin-react": "^4.3.1",
    "autoprefixer": "^10.4.18",
    "concurrently": "^9.1.0",
    "cross-env": "^7.0.3",
    "eslint": "^9.9.1",
    "eslint-plugin-react-hooks": "^5.1.0-rc.0",
    "eslint-plugin-react-refresh": "^0.4.11",
    "glob": "^9.3.5",
    "globals": "^15.9.0",
    "husky": "^9.1.7",
    "lint-staged": "^15.2.2",
    "postcss": "^8.4.35",
    "prettier": "^3.2.5",
    "rollup-plugin-visualizer": "^5.12.0",
    "tailwindcss": "^3.4.1",
    "terser": "^5.36.0",
    "ts-node": "^10.9.2",
    "tsx": "^4.19.2",
    "typescript": "^5.6.3",
    "typescript-eslint": "^8.3.0",
    "vite": "^5.4.2"
  }
